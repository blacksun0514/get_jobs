<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="boss.BossConfigController"
            stylesheets="@../css/modern-style.css"
            style="-fx-background-color: linear-gradient(to bottom, #f8fafc, #e2e8f0);">
    
    <!-- Header -->
    <top>
        <VBox style="-fx-background-color: linear-gradient(to right, #667eea, #764ba2); -fx-padding: 20;">
            <Label text="Boss直聘配置" 
                   style="-fx-text-fill: white; -fx-font-size: 28px; -fx-font-weight: bold;"/>
            <Label text="智能求职助手 - 让投递更高效" 
                   style="-fx-text-fill: rgba(255,255,255,0.9); -fx-font-size: 14px; -fx-padding: 5 0 0 0;"/>
        </VBox>
    </top>
    
    <!-- Main Content -->
    <center>
        <HBox spacing="20" style="-fx-padding: 20;">
            
            <!-- Left Panel: Basic Configuration -->
            <VBox spacing="16" HBox.hgrow="ALWAYS" 
                  style="-fx-background-color: white; -fx-background-radius: 12; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.1), 15, 0, 0, 5); -fx-padding: 24;">
                
                <Label text="🔧 基本配置" 
                       style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #2d3748;"/>
                
                <ScrollPane fitToWidth="true" style="-fx-background-color: transparent;">
                    <VBox spacing="20">
                        
                        <!-- Search Configuration Card -->
                        <VBox spacing="16" 
                              style="-fx-background-color: #f7fafc; -fx-background-radius: 8; -fx-padding: 16; -fx-border-color: #e2e8f0; -fx-border-radius: 8;">
                            <Label text="🎯 搜索条件" 
                                   style="-fx-font-size: 16px; -fx-font-weight: bold; -fx-text-fill: #4a5568;"/>
                            
                            <!-- Keywords -->
                            <VBox spacing="8">
                                <Label text="搜索关键词" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                    <tooltip>
                                        <Tooltip text="输入职位搜索关键词，多个关键词请用英文逗号分隔，例如：Java,Spring,后端" />
                                    </tooltip>
                                </Label>
                                <TextField fx:id="keywordsField" 
                                          promptText="Java, Spring Boot, 微服务"
                                          style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0; -fx-font-size: 14px;"/>
                            </VBox>
                            
                            <!-- Industry -->
                            <VBox spacing="8">
                                <Label text="目标行业" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                    <tooltip>
                                        <Tooltip text="选择目标公司所属行业，可多选，例如：互联网,金融,教育" />
                                    </tooltip>
                                </Label>
                                <TextField fx:id="industryField" 
                                          promptText="互联网, 金融科技, 人工智能"
                                          style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0; -fx-font-size: 14px;"/>
                            </VBox>
                            
                            <!-- City -->
                            <VBox spacing="8">
                                <Label text="工作城市" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                    <tooltip>
                                        <Tooltip text="输入期望工作的城市，多个城市用逗号分隔，例如：北京,上海,广州" />
                                    </tooltip>
                                </Label>
                                <TextField fx:id="cityCodeField" 
                                          promptText="北京, 上海, 深圳"
                                          style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0; -fx-font-size: 14px;"/>
                            </VBox>
                        </VBox>
                        
                        <!-- Job Requirements Card -->
                        <VBox spacing="16" 
                              style="-fx-background-color: #f7fafc; -fx-background-radius: 8; -fx-padding: 16; -fx-border-color: #e2e8f0; -fx-border-radius: 8;">
                            <Label text="📋 职位要求" 
                                   style="-fx-font-size: 16px; -fx-font-weight: bold; -fx-text-fill: #4a5568;"/>
                            
                            <GridPane hgap="16" vgap="16">
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                </columnConstraints>
                                
                                <!-- Experience -->
                                <VBox spacing="8" GridPane.columnIndex="0" GridPane.rowIndex="0">
                                    <Label text="工作经验" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                        <tooltip>
                                            <Tooltip text="选择您的工作年限区间" />
                                        </tooltip>
                                    </Label>
                                    <ComboBox fx:id="experienceComboBox" maxWidth="Infinity"
                                             style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 8;"/>
                                </VBox>
                                
                                <!-- Job Type -->
                                <VBox spacing="8" GridPane.columnIndex="1" GridPane.rowIndex="0">
                                    <Label text="求职类型" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                        <tooltip>
                                            <Tooltip text="选择期望的工作性质，如全职、兼职等" />
                                        </tooltip>
                                    </Label>
                                    <ComboBox fx:id="jobTypeComboBox" maxWidth="Infinity"
                                             style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 8;"/>
                                </VBox>
                                
                                <!-- Salary Range -->
                                <VBox spacing="8" GridPane.columnIndex="0" GridPane.rowIndex="1">
                                    <Label text="薪资范围" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                        <tooltip>
                                            <Tooltip text="选择期望的月薪范围" />
                                        </tooltip>
                                    </Label>
                                    <ComboBox fx:id="salaryComboBox" maxWidth="Infinity"
                                             style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 8;"/>
                                </VBox>
                                
                                <!-- Education -->
                                <VBox spacing="8" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                    <Label text="学历要求" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                        <tooltip>
                                            <Tooltip text="选择您的最高学历" />
                                        </tooltip>
                                    </Label>
                                    <ComboBox fx:id="degreeComboBox" maxWidth="Infinity"
                                             style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 8;"/>
                                </VBox>
                                
                                <!-- Company Scale -->
                                <VBox spacing="8" GridPane.columnIndex="0" GridPane.rowIndex="2">
                                    <Label text="公司规模" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                        <tooltip>
                                            <Tooltip text="选择期望的公司人员规模" />
                                        </tooltip>
                                    </Label>
                                    <ComboBox fx:id="scaleComboBox" maxWidth="Infinity"
                                             style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 8;"/>
                                </VBox>
                                
                                <!-- Funding Stage -->
                                <VBox spacing="8" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                    <Label text="融资阶段" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                        <tooltip>
                                            <Tooltip text="选择期望公司的融资阶段，如未融资、A轮、B轮等" />
                                        </tooltip>
                                    </Label>
                                    <ComboBox fx:id="stageComboBox" maxWidth="Infinity"
                                             style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 8;"/>
                                </VBox>
                            </GridPane>
                            
                            <!-- Expected Salary -->
                            <VBox spacing="8">
                                <Label text="期望薪资 (K)" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                    <tooltip>
                                        <Tooltip text="输入期望的薪资范围（单位：K）" />
                                    </tooltip>
                                </Label>
                                <HBox spacing="12" alignment="CENTER_LEFT">
                                    <TextField fx:id="minSalaryField" promptText="15" prefWidth="100"
                                              style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0;"/>
                                    <Label text="~" style="-fx-font-size: 16px; -fx-font-weight: bold; -fx-text-fill: #4a5568;"/>
                                    <TextField fx:id="maxSalaryField" promptText="30" prefWidth="100"
                                              style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0;"/>
                                </HBox>
                            </VBox>
                        </VBox>
                        
                        <!-- Resume Configuration Card -->
                        <VBox spacing="16" 
                              style="-fx-background-color: #f7fafc; -fx-background-radius: 8; -fx-padding: 16; -fx-border-color: #e2e8f0; -fx-border-radius: 8;">
                            <Label text="📄 简历配置" 
                                   style="-fx-font-size: 16px; -fx-font-weight: bold; -fx-text-fill: #4a5568;"/>
                            
                            <!-- Resume Image Path -->
                            <VBox spacing="8">
                                <Label text="简历图片路径" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                    <tooltip>
                                        <Tooltip text="设置简历图片的完整文件路径，支持常见图片格式（jpg、png、gif等）" />
                                    </tooltip>
                                </Label>
                                <TextField fx:id="resumeImagePathField" 
                                          promptText="D:\resume\my_resume.jpg"
                                          style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0; -fx-font-size: 14px;"/>
                            </VBox>
                            
                            <!-- Resume Content -->
                            <VBox spacing="8">
                                <Label text="简历内容" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                    <tooltip>
                                        <Tooltip text="设置详细的简历内容，包括个人信息、技能、工作经历、项目经验等" />
                                    </tooltip>
                                </Label>
                                <TextArea fx:id="resumeContentTextArea" wrapText="true" prefRowCount="6" 
                                         promptText="在此输入详细的简历内容..."
                                         style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0; -fx-font-size: 14px;"/>
                            </VBox>
                            
                            <!-- Greeting Content -->
                            <VBox spacing="8">
                                <Label text="打招呼内容" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                    <tooltip>
                                        <Tooltip text="设置与HR打招呼时的自动发送内容，建议个性化定制，增加回复率" />
                                    </tooltip>
                                </Label>
                                <TextArea fx:id="sayHiTextArea" wrapText="true" prefRowCount="4"
                                         style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0; -fx-font-size: 14px;"/>
                            </VBox>
                        </VBox>
                        
                        <!-- Advanced Configuration Accordion -->
                        <Accordion>
                            <panes>
                                <TitledPane text="🔧 高级配置" 
                                           style="-fx-background-color: #edf2f7; -fx-background-radius: 8;">
                                    <VBox spacing="16" style="-fx-padding: 16;">
                                        <!-- Custom City Codes -->
                                        <VBox spacing="8">
                                            <Label text="补充城市代码" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                                <tooltip>
                                                    <Tooltip text="添加自定义的城市代码映射关系" />
                                                </tooltip>
                                            </Label>
                                            <VBox fx:id="customCityCodeContainer" spacing="8" 
                                                  style="-fx-border-color: #cbd5e0; -fx-border-radius: 8; -fx-padding: 12; -fx-background-color: white;">
                                                <Label text="格式：城市名 - 城市代码" style="-fx-text-fill: #718096; -fx-font-style: italic;" />
                                            </VBox>
                                        </VBox>

                                        <!-- HR Status Configuration -->
                                        <VBox spacing="8">
                                            <Label text="HR状态配置" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                                                <tooltip>
                                                    <Tooltip text="配置HR活跃度判断规则" />
                                                </tooltip>
                                            </Label>
                                            <VBox fx:id="deadStatusContainer" spacing="8" 
                                                  style="-fx-border-color: #cbd5e0; -fx-border-radius: 8; -fx-padding: 12; -fx-background-color: white;">
                                                <Label text="可添加或删除HR状态" style="-fx-text-fill: #718096; -fx-font-style: italic;" />
                                            </VBox>
                                        </VBox>
                                    </VBox>
                                </TitledPane>
                            </panes>
                        </Accordion>
                    </VBox>
                </ScrollPane>
            </VBox>
            
            <!-- Middle Panel: Settings & AI Configuration -->
            <VBox spacing="16" HBox.hgrow="ALWAYS" 
                  style="-fx-background-color: white; -fx-background-radius: 12; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.1), 15, 0, 0, 5); -fx-padding: 24;">
                
                <Label text="⚙️ 智能设置" 
                       style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #2d3748;"/>
                
                <!-- Feature Toggles Card -->
                <VBox spacing="16" 
                      style="-fx-background-color: #ebf8ff; -fx-background-radius: 8; -fx-padding: 16; -fx-border-color: #bee3f8; -fx-border-radius: 8;">
                    <Label text="🎛️ 功能开关" 
                           style="-fx-font-size: 16px; -fx-font-weight: bold; -fx-text-fill: #2b6cb0;"/>
                    
                    <GridPane hgap="20" vgap="16">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        </columnConstraints>
                        
                        <HBox spacing="12" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="0">
                            <CheckBox fx:id="filterDeadHRCheckBox" 
                                     style="-fx-text-fill: #2d3748; -fx-font-weight: 500;"/>
                            <Label text="过滤不活跃HR" style="-fx-text-fill: #2d3748; -fx-font-weight: 500;">
                                <tooltip>
                                    <Tooltip text="开启后将自动过滤最近未活跃的HR，提高沟通效率" />
                                </tooltip>
                            </Label>
                        </HBox>
                        
                        <HBox spacing="12" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="1">
                            <CheckBox fx:id="sendImgResumeCheckBox" 
                                     style="-fx-text-fill: #2d3748; -fx-font-weight: 500;"/>
                            <Label text="发送图片简历" style="-fx-text-fill: #2d3748; -fx-font-weight: 500;">
                                <tooltip>
                                    <Tooltip text="开启后将自动发送图片版简历，提高简历查看率" />
                                </tooltip>
                            </Label>
                        </HBox>
                        
                        <HBox spacing="12" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="2">
                            <CheckBox fx:id="keyFilterCheckBox" 
                                     style="-fx-text-fill: #2d3748; -fx-font-weight: 500;"/>
                            <Label text="关键词智能匹配" style="-fx-text-fill: #2d3748; -fx-font-weight: 500;">
                                <tooltip>
                                    <Tooltip text="开启后将根据设定的关键词智能匹配职位要求" />
                                </tooltip>
                            </Label>
                        </HBox>
                        
                        <HBox spacing="12" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="3">
                            <CheckBox fx:id="recommendJobsCheckBox" 
                                     style="-fx-text-fill: #2d3748; -fx-font-weight: 500;"/>
                            <Label text="接收推荐岗位" style="-fx-text-fill: #2d3748; -fx-font-weight: 500;">
                                <tooltip>
                                    <Tooltip text="开启后将自动接收系统推荐的相关职位" />
                                </tooltip>
                            </Label>
                        </HBox>
                        
                        <HBox spacing="12" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="4">
                            <CheckBox fx:id="h5JobsCheckBox" selected="false" disable="true" 
                                     style="-fx-text-fill: #a0aec0; -fx-font-weight: 500;"/>
                            <Label text="H5岗位 (已禁用)" style="-fx-text-fill: #a0aec0; -fx-font-weight: 500;">
                                <tooltip>
                                    <Tooltip text="H5端发布的职位（功能已禁用）" />
                                </tooltip>
                            </Label>
                        </HBox>
                    </GridPane>
                </VBox>
                
                <!-- AI Configuration Card -->
                <VBox spacing="16" 
                      style="-fx-background-color: #f0fff4; -fx-background-radius: 8; -fx-padding: 16; -fx-border-color: #9ae6b4; -fx-border-radius: 8;">
                    <Label text="🤖 AI智能配置" 
                           style="-fx-font-size: 16px; -fx-font-weight: bold; -fx-text-fill: #276749;"/>
                    
                    <GridPane hgap="20" vgap="16">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        </columnConstraints>
                        
                        <HBox spacing="12" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="0">
                            <CheckBox fx:id="enableAIJobMatchDetectionCheckBox" 
                                     style="-fx-text-fill: #2d3748; -fx-font-weight: 500;"/>
                            <Label text="AI岗位匹配检测" style="-fx-text-fill: #2d3748; -fx-font-weight: 500;">
                                <tooltip>
                                    <Tooltip text="开启后将使用AI智能分析职位描述，检测岗位与期望的匹配度" />
                                </tooltip>
                            </Label>
                        </HBox>
                        
                        <HBox spacing="12" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="1">
                            <CheckBox fx:id="enableAIGreetingCheckBox" 
                                     style="-fx-text-fill: #2d3748; -fx-font-weight: 500;"/>
                            <Label text="AI智能打招呼" style="-fx-text-fill: #2d3748; -fx-font-weight: 500;">
                                <tooltip>
                                    <Tooltip text="开启后将使用AI智能生成个性化的打招呼内容，提高HR回复率" />
                                </tooltip>
                            </Label>
                        </HBox>
                        
                        <HBox spacing="12" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="2">
                            <CheckBox fx:id="checkStateOwnedCheckBox" 
                                     style="-fx-text-fill: #2d3748; -fx-font-weight: 500;"/>
                            <Label text="国企识别检查" style="-fx-text-fill: #2d3748; -fx-font-weight: 500;">
                                <tooltip>
                                    <Tooltip text="开启后将自动识别并标记国有企业职位" />
                                </tooltip>
                            </Label>
                        </HBox>
                    </GridPane>
                    
                    <!-- VIP Key -->
                    <VBox spacing="8">
                        <Label text="VIP密钥" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                            <tooltip>
                                <Tooltip text="输入VIP授权密钥，用于解锁高级功能" />
                            </tooltip>
                        </Label>
                        <TextField fx:id="vipKeyField" 
                                  promptText="请输入VIP授权密钥"
                                  style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0; -fx-font-size: 14px;"/>
                    </VBox>
                    
                    <!-- API Domain -->
                    <VBox spacing="8">
                        <Label text="API服务域名" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                            <tooltip>
                                <Tooltip text="设置API服务器地址，一般无需修改" />
                            </tooltip>
                        </Label>
                        <TextField fx:id="apiDomainField" 
                                  promptText="http://127.0.0.1:9090"
                                  style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0; -fx-font-size: 14px;"/>
                    </VBox>
                </VBox>
                
                <!-- System Configuration Card -->
                <VBox spacing="16" 
                      style="-fx-background-color: #fef5e7; -fx-background-radius: 8; -fx-padding: 16; -fx-border-color: #f6e05e; -fx-border-radius: 8;">
                    <Label text="🛠️ 系统参数" 
                           style="-fx-font-size: 16px; -fx-font-weight: bold; -fx-text-fill: #744210;"/>
                    
                    <!-- Debug Mode -->
                    <HBox spacing="12" alignment="CENTER_LEFT">
                        <CheckBox fx:id="debuggerCheckBox" 
                                 style="-fx-text-fill: #2d3748; -fx-font-weight: 500;"/>
                        <Label text="调试模式" style="-fx-text-fill: #2d3748; -fx-font-weight: 500;">
                            <tooltip>
                                <Tooltip text="开启后将显示详细的运行日志，方便排查问题" />
                            </tooltip>
                        </Label>
                    </HBox>
                    
                    <!-- Wait Time -->
                    <VBox spacing="8">
                        <Label text="操作间隔时间 (秒)" style="-fx-font-weight: 600; -fx-text-fill: #2d3748;">
                            <tooltip>
                                <Tooltip text="设置每次操作之间的等待时间，建议设置在10-30秒之间" />
                            </tooltip>
                        </Label>
                        <TextField fx:id="waitTimeField" 
                                  promptText="10"
                                  style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-padding: 12; -fx-border-color: #cbd5e0; -fx-font-size: 14px;"/>
                    </VBox>
                </VBox>
                
                <!-- Action Button -->
                <Button text="🚀 保存配置并开始执行" onAction="#handleSaveAndStart" 
                        maxWidth="Infinity"
                        style="-fx-background-color: linear-gradient(to right, #48bb78, #38a169); -fx-text-fill: white; -fx-font-size: 16px; -fx-font-weight: bold; -fx-background-radius: 12; -fx-padding: 16; -fx-effect: dropshadow(gaussian, rgba(72,187,120,0.3), 10, 0, 0, 3);">
                    <tooltip>
                        <Tooltip text="保存当前配置并开始执行自动投递任务" />
                    </tooltip>
                </Button>
            </VBox>
            
            <!-- Right Panel: Console Log -->
            <VBox spacing="16" HBox.hgrow="ALWAYS" 
                  style="-fx-background-color: white; -fx-background-radius: 12; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.1), 15, 0, 0, 5); -fx-padding: 24;">
                
                <HBox spacing="12" alignment="CENTER_LEFT">
                    <Label text="📊 运行日志" 
                           style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #2d3748;"/>
                    <Region HBox.hgrow="ALWAYS"/>
                    <Label text="实时监控" 
                           style="-fx-background-color: #48bb78; -fx-text-fill: white; -fx-padding: 4 8; -fx-background-radius: 12; -fx-font-size: 12px; -fx-font-weight: bold;"/>
                </HBox>
                
                <TextArea fx:id="logTextArea" 
                          wrapText="true" 
                          editable="false" 
                          VBox.vgrow="ALWAYS"
